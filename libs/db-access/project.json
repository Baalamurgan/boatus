{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/db-access/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/db-access/**/*.ts"
        ]
      }
    },
    "migrate-dev": {
      "builder": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "npx prisma migrate dev --schema ./src/prisma/schema.prisma",
        "cwd": "libs/db-access"
      }
    },
    "push": {
      "builder": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "npx prisma migrate push --schema ./src/prisma/schema.prisma",
        "cwd": "libs/db-access"
      }
    },
    "migrate-up": {
      "builder": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "npx prisma migrate up --schema ./src/prisma/schema.prisma",
        "cwd": "libs/db-access"
      }
    },
    "gen": {
      "builder": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "npx prisma generate --schema ./src/prisma/schema.prisma",
        "cwd": "libs/db-access"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/db-access"
      ],
      "options": {
        "jestConfig": "libs/db-access/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}